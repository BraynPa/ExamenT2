@{ List<PokeUser> pokeser = ((List<PokeUser>)Model); }

<h1>Pokemon </h1>
<h3>Total de Pokemones: @pokeser.Count </h3>


<table class="table">
    <thead>
        <tr>
            <th>IdPokemon</th>
            <th>Nombre</th>
            <th>Tipo</th>
            <th>Fecha de Captura</th>
            <th>Imagen</th>
            <th></th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in pokeser)
        {
<tr>
    <td>@item.IdPokemon</td>
    <td>@item.Pokemon.Nombre</td>
    <td>@item.Pokemon.Type.Nombre</td>
    <td>@item.FechaCaptura</td>
    <td>
        <img src="@item.Pokemon.RutaImgPo" width="100" height="20" alt="Alternate Text" />
    </td>
    <td>
        <a href="/Pokemon/Liberar?PokemonId=@item.IdPokemon">Liberar</a>
    </td>
</tr>
}
    </tbody>
</table>